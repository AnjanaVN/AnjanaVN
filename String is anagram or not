#include<stdio.h>
#include<string.h>
void main()
{
char str[50],b[50];
int i,j,count=0,charc=0;
printf("enter string1");
scanf("%s",&str);
printf("enter string 2");
scanf("%s",&b);
for(i=0;str[i];i++)
{
charc++;
}
for(i=0;str[i];i++)
for(j=0;b[j];j++)
if(str[i]==b[j])
count++;
if(count==charc)
printf("anagram");
else
printf("not anagram");

}
